{
  "name": "larryli/ipv4-yii2",
  "description": "IP v4 中国城市地址库 Yii2 组件",
  "homepage": "https://github.com/larryli/ipv4-yii2",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Larry Li",
      "email": "larryli@qq.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "larryli/ipv4": "~1.0",
    "yiisoft/yii2": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": { "larryli\\ipv4\\yii2\\": "" }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
